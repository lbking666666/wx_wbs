.My{
  background:url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/home-bg.png) no-repeat;
  background-size:100% auto;
  width:100%;
}
.My .My-up{
  padding-top: 40rpx;
}
.My .My-up .card-box{
  height: 380rpx;
  width: 690rpx;
  border-radius: 20rpx;
  overflow: hidden;
  box-shadow: 0 5rpx 12rpx rgba(0, 0, 0, 0.2)
}
.My .My-up .info-card{
  margin: 0 30rpx;
}
.My .My-up .info-card.normal{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/usercard-normal.png) no-repeat;
  background-size: 100% 100%;
}
.My .My-up .normal .d-font{
  color: #888;
}
.My .My-up .normal .s-font{
  color: #cda87a;
}
.My .My-up .normal .p-1{
  color: #333;
}
.My .My-up .normal .level-icon{
  border: 1px solid #888;
  color: #888;
}

.My .My-up .info-card.silver{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/usercard-silver.png) no-repeat;
  background-size: 100% 100%;
}
.My .My-up .silver .d-font{
  color: #666;
}
.My .My-up .silver .s-font{
  color: #fff;
}
.My .My-up .silver .level-icon{
  border: 1px solid #666;
  color: #666;
}

.My .My-up .info-card.gold{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/usercard-gold.png) no-repeat;
  background-size: 100% 100%;
}
.My .My-up .gold .d-font{
  color: #927f4b;
}
.My .My-up .gold .s-font{
  color: #fff;
}
.My .My-up .gold .level-icon{
  border: 1px solid #927f4b;
  color: #927f4b;
}

.My .My-up .info-card.platinum{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/usercard-platinum.png) no-repeat;
  background-size: 100% 100%;
}
.My .My-up .platinum .d-font{
  color: #6f97ab;
}
.My .My-up .platinum .s-font{
  color: #fff;
}
.My .My-up .platinum .level-icon{
  border: 1px solid #6f97ab;
  color: #6f97ab;
}

.My-up .info-card .lf-box{
  float: left;
  padding: 50rpx 30rpx 0 40rpx;
}
.My-up .info-card .lf-box .head-img{
  width: 120rpx;
  height: 120rpx;
  border: 3rpx solid #fff;
  border-radius: 63rpx;
  overflow: hidden;
}
.My-up .info-card .rt-box{
  float: left;
  width: 485rpx;
  padding: 69rpx 0 0 0;
}
.My-up .info-card .rt-box .p-1{
  text-align: left;
  height: 38rpx;
  line-height: 38rpx;
  font-weight: 700;
}
.info-card .rt-box .p-1 .level-icon{
  font-weight: normal;
  display: inline-block;
  padding: 0 9rpx;
  height: 28rpx;
  line-height: 28rpx;
  font-size: 20rpx;
  text-align: center;
  border-radius: 5rpx;
  vertical-align: middle;
  margin-left: 18rpx;
}
.info-card .rt-box .p-2{
  text-align: left;
  margin-top: 24rpx;
  height: 28rpx;
  line-height: 28rpx;
  font-size: 28rpx;
}
.info-card .rt-box .p-2 .v-icon{
  display: inline-block;
  width: 25rpx;
  height: 23rpx;
}
.info-card .rt-box .p-2 .diamond-icon{
  display: inline-block;
  width: 26rpx;
  height: 23rpx;
  margin-left: 40rpx;
}
.normal .rt-box .p-2 .v-icon{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/v1.png) no-repeat;
  background-size: 100% 100%;
}
.normal .rt-box .p-2 .diamond-icon{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/diamond1.png) no-repeat;
  background-size: 100% 100%;
}
.silver .rt-box .p-2 .v-icon{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/v2.png) no-repeat;
  background-size: 100% 100%;
}
.silver .rt-box .p-2 .diamond-icon{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/diamond2.png) no-repeat;
  background-size: 100% 100%;
}
.gold .rt-box .p-2 .v-icon{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/v3.png) no-repeat;
