  background-size: 100% 100%;
}
.platinum .rt-box .p-2 .v-icon{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/v4.png) no-repeat;
  background-size: 100% 100%;
}
.platinum .rt-box .p-2 .diamond-icon{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/diamond4.png) no-repeat;
  background-size: 100% 100%;
}
.info-card .rt-box .p-3{
  text-align: left;
  margin-top: 54rpx;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 60rpx;
}
.info-card .rt-box .p-3 .hide-points{
  display: inline-block;
  vertical-align:top;
  width:250rpx;
  height:25rpx;
  margin-top:18rpx;
}
.silver .rt-box .p-3 .hide-points,
.gold .rt-box .p-3 .hide-points,
.platinum .rt-box .p-3 .hide-points{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/white-hidepoints.png) no-repeat;
  background-size: 100% 100%;
}
.normal .rt-box .p-3 .hide-points{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/gold-hidepoints.png) no-repeat;
  background-size: 100% 100%;
}
.info-card .rt-box .p-3 .eye{
  display: inline-block;
  vertical-align:top;
  margin-left:20rpx;
  width:40rpx;
  height:29rpx;
  margin-top:16rpx;
}
.silver .rt-box .p-3 .open-eye,
.gold .rt-box .p-3 .open-eye,
.platinum .rt-box .p-3 .open-eye{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/white-closeeye.png) no-repeat;
  background-size: 100% 100%;
}
.silver .rt-box .p-3 .close-eye,
.gold .rt-box .p-3 .close-eye,
.platinum .rt-box .p-3 .close-eye{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/white-openeye.png) no-repeat;
  background-size: 100% 100%;
}
.normal .rt-box .p-3 .open-eye{
  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/gold-closeeye.png) no-repeat;
