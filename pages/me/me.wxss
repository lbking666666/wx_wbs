  background: url(http://xm-file.oss-cn-beijing.aliyuncs.com/wchat/gold-openeye.png) no-repeat;
  background-size: 100% 100%;
}

.info-card .rt-box .p-4{
  text-align: left;
  line-height: 28rpx;
  font-size: 28rpx;
  margin-top: 16rpx;
}
